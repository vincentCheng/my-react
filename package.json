{
  "name": "my-react",
  "version": "1.0.0",
  "description": "手动搭建react脚手架",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "ts:checker": "tsc --noEmit",
    "dev": "cross-env NODE_ENV=development webpack serve --config scripts/webpack.dev.js --progress --hot",
    "build": "npx tsc --noEmit && cross-env NODE_ENV=production webpack -c scripts/webpack.prod.js",
    "test": "jest",
    "tw": "jest --watchAll",
    "prepublish": "npm version && npm run build",
    "prepare": "husky install"
  },
  "author": "VincentHuang",
  "license": "MIT",
  "browerslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "defaults",
      "not ie < 11",
      "last 2 version",
      "> 1%",
      "iOS 9",
      "last 3 iOS versions"
    ]
  },
  "huskey": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKEY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/.bin/prettier -w",
      "node --max_old_space_size=8192 ./node_modules/.bin/eslint --fix --color",
      "git add"
    ]
  },
  "dependencies": {
    "@arco-design/web-react": "^2.45.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.19",
    "axios": "^1.3.4",
    "axios-retry": "^3.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@arco-plugins/webpack-react": "^1.4.8",
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "chalk": "^4.1.2",
    "core-js": "^3.29.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.4",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^4.2.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
